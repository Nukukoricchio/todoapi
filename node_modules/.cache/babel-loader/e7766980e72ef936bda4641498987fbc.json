markComplete\">\n            <i class=\"fa fa-check\" v-show=\"todo.completed\"></i\n            ><span> &nbsp;</span> <span> &nbsp;</span>{{ todo.title }}\n          </p>\n        </div>\n      </div>\n      <form v-else class=\"flex-grow-1\" @submit.prevent=\"finishEditing()\">\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          v-model=\"newTitle\"\n          @blur=\"finishEditing()\"\n          ref=\"newTodo\"\n        />\n      </form>\n      <div class=\"widget-content-right\">\n        <button\n          class=\"border-0 btn-transition btn btn-outline-success\"\n          v-on:click=\"startEditing()\"\n        >\n          <i class=\"fa fa-edit\"></i>\n        </button>\n        <button\n          @click=\"$emit('delete-todo', todo.title)\"\n          class=\"border-0 btn-transition btn btn-outline-danger\"\n        >\n          <i class=\"fa fa-trash\"></i>\n        </button>\n      </div>\n    </div>\n    <hr />\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Todo\",\n  props: [\"todo\"],\n  data() {\n    return {\n      isEditing: false,\n      newTitle: \"\",\n    };\n  },\n  methods: {\n    markComplete: function() {\n      this.todo.completed = !this.todo.completed;\n    },\n    startEditing() {\n      if (this.isEditing) {\n        this.finishEditing();\n      } else {\n        this.newTitle = this.todo.title;\n        this.isEditing = true;\n        this.$nextTick(() => this.$refs.newTodo.focus());\n      }\n    },\n    finishEditing() {\n      this.isEditing = false;\n      this.$emit(\"on-edit\", this.newTitle);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.completed {\n  text-decoration: line-through;\n}\n</style>\n"],"sourceRoot":"src/components"}]}